{
  "name": "vscode-plugin",
  "publisher": "scann-oss",
  "displayName": "scanoss-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.scanCurrentFile",
        "title": "ScanOSS: Scan Current File",
        "enablement": "resource"
      },
      {
        "command": "extension.scanProject",
        "title": "ScanOSS: Scan Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint \"./**/*.{ts,tsx}\" --fix",
    "type-check": "tsc --noEmit",
    "test": "echo 'no tests, yet... ðŸ˜ˆ",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged",
    "clear:node-modules": "npm exec -- npx rimraf node_modules && npx rimraf node_modules",
    "clear:builds": "npm exec -- npx rimraf out",
    "clear:project": "pnpm run clear:node-modules && pnpm run clear:builds",
    "clear:project-and-install": "pnpm run clear:project && pnpm run"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "commitlint-plugin-workspace-scopes": "^1.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-turbo": "latest",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cosmiconfig": "^8.1.3",
    "scanoss": "^0.8.8",
    "snyk-nodejs-lockfile-parser": "^1.48.3"
  }
}
